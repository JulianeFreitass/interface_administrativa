<%-include('base_header') -%>
    <ul>
    <% for(var i=0; i< lista_projetos.length; ++i) { %>        
        <li> 
            <span class="descritor">Título do Projeto:</span> <%=lista_projetos[i].titulo %><br/>
            <span class="descritor">link do Projeto:</span> <%=lista_projetos[i].link %><br/>
